{
  "name": "june-ui",
  "version": "0.1.5",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/june-ui.min.js",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "description": "",
  "scripts": {
    "commit": "git-cz",
    "dev": "docz dev",
    "start": "cross-env BABEL_ENV=preview webpack-dev-server  --port 3002 --open --config scripts/webpack.config.dev.js",
    "build:prod": "cross-env BABEL_ENV=preview webpack --mode production --config scripts/webpack.config.prod.js",
    "deploy:docs": "yarn build:prod && gh-pages -d docs",
    "build:doc": "rimraf doc-site && docz build",
    "preview:doc": "docz serve",
    "build:css": "gulp compile",
    "build:umd": "NODE_ENV=production webpack --mode production --config scripts/webpack.config.umd.js",
    "build:umd-dev": "webpack --mode development --config scripts/webpack.config.umd.js",
    "build:commonjs": "rimraf lib && cross-env BABEL_ENV=commonjs babel src/components -d lib --source-maps",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src/components -d es --source-maps",
    "build:all": "yarn build:umd && yarn build:commonjs && yarn build:es && yarn build:css"
  },
  "keywords": [],
  "author": "guojunwei <junewill2019@qq.com> (https://github.com/guojw2315)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-modules-umd": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@umijs/fabric": "^2.2.2",
    "antd": "^4.6.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "docz": "^2.3.1",
    "fs": "^0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-filesize": "^0.0.6",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.3.0",
    "less-loader": "^7.0.1",
    "lint-staged": "^10.3.0",
    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "prettier": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^4.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-md5-hash": "^0.0.6",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}
